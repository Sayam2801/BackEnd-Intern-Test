<%- include('partials/header') -%>

<h1 class="title">Welcome To The User Registration Page!</h1>
<div id="outer">
    <div class="inner">
      <form action="/register" method="POST">
          <button class="btn btn-warning home-but" type="submit" name="button">Sign Up</button>
      </form>
    </div>
    <div class="inner">
      <form action="/signinOption" method="POST">
          <button class="btn btn-danger home-but" type="submit" name="button">Sign In</button>
      </form>
    </div>
    <div class="inner">
      <form action="/getDetails" method="POST">
          <button class="btn btn-primary home-but" type="submit" name="button">Get Details</button>
      </form>
    </div>
</div>
<%- include('partials/footer') -%>
